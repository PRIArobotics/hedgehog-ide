{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": "single",
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "curly": false,
    "no-consecutive-blank-lines": false,
    "whitespace": [false, "check-branch"]
  },
  "rulesDirectory": [
    "node_modules/codelyzer"
  ]
}
